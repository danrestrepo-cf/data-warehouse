  <step>
    <name>[load_mdi_configuration] Load settings based on process_name parameter</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Config DB Connection</connection>
    <sql>SELECT
    p.name AS process_name
    , p.dwid AS process_dwid
    , CASE
          WHEN csv_file_input_step.process_dwid IS NOT NULL THEN 'CSV'
          WHEN csv_file_input_step.process_dwid IS NULL THEN 'EXCEL'
          ELSE 'No input steps are configured for ' || p.name
      END AS input_template
    , coalesce( csv_file_input_step.data_source_dwid,
                microsoft_excel_input_step.data_source_dwid ) AS data_source_dwid
FROM
    mdi.process p
        LEFT JOIN mdi.csv_file_input_step ON p.dwid = csv_file_input_step.process_dwid
        LEFT JOIN mdi.table_output_step ON p.dwid = table_output_step.process_dwid
        LEFT JOIN mdi.microsoft_excel_input_step ON p.dwid = microsoft_excel_input_step.process_dwid
WHERE
    p.name ='${process_name}'
;
</sql>
