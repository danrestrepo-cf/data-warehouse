{
    "Comment": "SP-100564 - ETL to copy lien_priority_type data from staging_octane to history_octane",
    "StartAt": "SP-100564",
    "States": {
        "SP-100564": {
            "Type": "Task",
            "Resource": "arn:aws:states:::ecs:runTask.waitForTaskToken",
            "Parameters": {
                "LaunchType": "FARGATE",
                "Cluster": "${ecsClusterARN}",
                "TaskDefinition": "${mdi_2_arn}",
                "NetworkConfiguration": {
                    "AwsvpcConfiguration": {
                        "AssignPublicIp": "DISABLED",
                        "SecurityGroups": [
                            "${securityGroupId}"
                        ],
                        "Subnets": ${subnetIDs}
                    }
                },
                "Overrides": {
                    "ContainerOverrides": [
                        {
                            "Environment": [
                                {
                                    "Name": "PROCESS_NAME",
                                    "Value": "SP-100564"
                                },
                                {
                                    "Name": "INPUT_DATA",
                                    "Value.$": "States.Format('\\{\"token_id\":\"{}\",\"mdi_input_json\":{}\\}', $$.Task.Token, States.JsonToString($))"
                                }
                            ],
                            "Name": "${mdi_2_container}"
                        }
                    ]
                }
            },
            "Next": "Choice"
        },
        "Choice": {
            "Type": "Choice",
            "Choices": [
                {
                    "Variable": "$.load_type",
                    "StringEquals": "NONE",
                    "Next": "Success"
                }
            ],
            "Default": "Parallel"
        },
        "Success": {
            "Type": "Succeed"
        },
        "Parallel": {
            "Type": "Parallel",
            "End": true,
            "Branches": [
                {
                    "Comment": "Send message to bi-managed-mdi-2-full-check-queue for SP-200014",
                    "StartAt": "SP-200014_message",
                    "States": {
                        "SP-200014_message": {
                            "Type": "Task",
                            "Resource": "arn:aws:states:::sqs:sendMessage",
                            "Parameters": {
                                "QueueUrl": "[QueueUrl]",
                                "MessageAttributes": {
                                    "MessageGroupId": {
                                        "DataType": "String",
                                        "StringValue": "loan_junk_dim"
                                    },
                                    "ProcessId": {
                                        "DataType": "String",
                                        "StringValue": "SP-200014"
                                    }
                                },
                                "MessageBody": {
                                    "PreviousStepOutput": "States.Format('\"mdi_input_json\":{}\\}', States.JsonToString($))"
                                }
                            },
                            "End": true
                        }
                    }
                },
                {
                    "Comment": "Send message to bi-managed-mdi-2-full-check-queue for SP-200018",
                    "StartAt": "SP-200018_message",
                    "States": {
                        "SP-200018_message": {
                            "Type": "Task",
                            "Resource": "arn:aws:states:::sqs:sendMessage",
                            "Parameters": {
                                "QueueUrl": "[QueueUrl]",
                                "MessageAttributes": {
                                    "MessageGroupId": {
                                        "DataType": "String",
                                        "StringValue": "product_terms_dim"
                                    },
                                    "ProcessId": {
                                        "DataType": "String",
                                        "StringValue": "SP-200018"
                                    }
                                },
                                "MessageBody": {
                                    "PreviousStepOutput": "States.Format('\"mdi_input_json\":{}\\}', States.JsonToString($))"
                                }
                            },
                            "End": true
                        }
                    }
                }
            ]
        }
    }
}

