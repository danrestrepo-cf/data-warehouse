{
    "Comment": "SP-100092 - ETL to copy deal_lender_user data from staging_octane to history_octane",
    "StartAt": "SP-100092",
    "States": {
        "SP-100092": {
            "Type": "Task",
            "Resource": "arn:aws:states:::ecs:runTask.waitForTaskToken",
            "Parameters": {
                "LaunchType": "FARGATE",
                "Cluster": "${ecsClusterARN}",
                "TaskDefinition": "${mdi_2_arn}",
                "NetworkConfiguration": {
                    "AwsvpcConfiguration": {
                        "AssignPublicIp": "DISABLED",
                        "SecurityGroups": [
                            "${securityGroupId}"
                        ],
                        "Subnets": ${subnetIDs}
                    }
                },
                "Overrides": {
                    "ContainerOverrides": [
                        {
                            "Environment": [
                                {
                                    "Name": "PROCESS_NAME",
                                    "Value": "SP-100092"
                                },
                                {
                                    "Name": "INPUT_DATA",
                                    "Value.$": "States.Format('\\{\"token_id\":\"{}\",\"mdi_input_json\":{}\\}', $$.Task.Token, States.JsonToString($))"
                                }
                            ],
                            "Name": "${mdi_2_container}"
                        }
                    ]
                }
            },
            "Next": "Choice"
        },
        "Choice": {
            "Type": "Choice",
            "Choices": [
                {
                    "Variable": "$.load_type",
                    "StringEquals": "NONE",
                    "Next": "Success"
                }
            ],
            "Default": "Parallel"
        },
        "Success": {
            "Type": "Succeed"
        },
        "Parallel": {
            "Type": "Parallel",
            "End": true,
            "Branches": [
                {
                    "Comment": "Send message to bi-managed-mdi-2-full-check-queue for SP-200001",
                    "StartAt": "SP-200001_message",
                    "States": {
                        "SP-200001_message": {
                            "Type": "Task",
                            "Resource": "arn:aws:states:::sqs:sendMessage",
                            "Parameters": {
                                "QueueUrl": "[QueueUrl]",
                                "MessageAttributes": {
                                    "MessageGroupId": {
                                        "DataType": "String",
                                        "StringValue": "loan_lender_user_access"
                                    },
                                    "ProcessId": {
                                        "DataType": "String",
                                        "StringValue": "SP-200001"
                                    }
                                },
                                "MessageBody": {
                                    "PreviousStepOutput": "States.Format('\"mdi_input_json\":{}\\}', States.JsonToString($))"
                                }
                            },
                            "End": true
                        }
                    }
                },
                {
                    "Comment": "Send message to bi-managed-mdi-2-full-check-queue for SP-200002",
                    "StartAt": "SP-200002_message",
                    "States": {
                        "SP-200002_message": {
                            "Type": "Task",
                            "Resource": "arn:aws:states:::sqs:sendMessage",
                            "Parameters": {
                                "QueueUrl": "[QueueUrl]",
                                "MessageAttributes": {
                                    "MessageGroupId": {
                                        "DataType": "String",
                                        "StringValue": "loan_lender_user_access"
                                    },
                                    "ProcessId": {
                                        "DataType": "String",
                                        "StringValue": "SP-200002"
                                    }
                                },
                                "MessageBody": {
                                    "PreviousStepOutput": "States.Format('\"mdi_input_json\":{}\\}', States.JsonToString($))"
                                }
                            },
                            "End": true
                        }
                    }
                }
            ]
        }
    }
}

